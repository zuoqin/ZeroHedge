@model ZeroHedge.Story

@{
    ViewBag.Title = @Model.Title;
}



<div class="row">
        <div class="col-sm-12">
          
          <div class="panel panel-primary">
            <div class="panel-heading">
              <h3 class="panel-title">@Html.Raw( @Model.Title )</h3>
            </div>
            <div class="panel-body">
              @Html.Raw( @Model.Body )
            </div>
          </div>
        </div><!-- /.col-sm-4 -->
      </div>

<div>
    <p>
        on @Model.Published
    </p>
</div>

<div>
        <p>
            <button type="button" class="btn btn-primary btn-mini" data-bind="click: backPage">Back to List</button>
        </p>
</div>

